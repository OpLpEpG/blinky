/dts-v1/;

/ {
	#address-cells = < 0x01 >;
	#size-cells = < 0x01 >;
	model = "STM32 Minimum Development Board (Blue)";
	compatible = "st,stm32_min_dev_blue", "st,stm32f103c8";

	chosen {
		zephyr,console = "/soc/serial@40013800";
		zephyr,shell-uart = "/soc/serial@40013800";
		zephyr,sram = "/memory@20000000";
		zephyr,flash = "/soc/flash-controller@40022000/flash@8000000";
	};

	aliases {
		led0 = "/leds/led";
	};

	soc {
		#address-cells = < 0x01 >;
		#size-cells = < 0x01 >;
		compatible = "simple-bus";
		interrupt-parent = < 0x01 >;
		ranges;

		nvic: interrupt-controller@e000e100 {
			compatible = "arm,v7m-nvic";
			reg = < 0xe000e100 0xc00 >;
			interrupt-controller;
			#interrupt-cells = < 0x02 >;
			arm,num-irq-priority-bits = < 0x04 >;
			phandle = < 0x01 >;
		};

		systick: timer@e000e010 {
			compatible = "arm,armv7m-systick";
			reg = < 0xe000e010 0x10 >;
			status = "disabled";
		};

		flash-controller@40022000 {
			compatible = "st,stm32f1-flash-controller";
			label = "FLASH_CTRL";
			reg = < 0x40022000 0x400 >;
			interrupts = < 0x03 0x00 >;
			#address-cells = < 0x01 >;
			#size-cells = < 0x01 >;

			flash0: flash@8000000 {
				compatible = "soc-nv-flash";
				label = "FLASH_STM32";
				write-block-size = < 0x02 >;
				reg = < 0x8000000 0x10000 >;
				erase-block-size = < 0x400 >;
			};
		};

		rcc: rcc@40021000 {
			compatible = "st,stm32-rcc";
			#clock-cells = < 0x02 >;
			reg = < 0x40021000 0x400 >;
			label = "STM32_CLK_RCC";
			phandle = < 0x02 >;
		};

		pinctrl: pin-controller@40010800 {
			compatible = "st,stm32-pinmux";
			#address-cells = < 0x01 >;
			#size-cells = < 0x01 >;
			reg = < 0x40010800 0x1c00 >;

			gpioa: gpio@40010800 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x02 >;
				reg = < 0x40010800 0x400 >;
				clocks = < 0x02 0x03 0x05 >;
				label = "GPIOA";
			};

			gpiob: gpio@40010c00 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x02 >;
				reg = < 0x40010c00 0x400 >;
				clocks = < 0x02 0x03 0x09 >;
				label = "GPIOB";
			};

			gpioc: gpio@40011000 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x02 >;
				reg = < 0x40011000 0x400 >;
				clocks = < 0x02 0x03 0x11 >;
				label = "GPIOC";
				phandle = < 0x04 >;
			};

			gpiod: gpio@40011400 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x02 >;
				reg = < 0x40011400 0x400 >;
				clocks = < 0x02 0x03 0x21 >;
				label = "GPIOD";
			};

			gpioe: gpio@40011800 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x02 >;
				reg = < 0x40011800 0x400 >;
				clocks = < 0x02 0x03 0x41 >;
				label = "GPIOE";
			};
		};

		usart1: serial@40013800 {
			compatible = "st,stm32-usart", "st,stm32-uart";
			reg = < 0x40013800 0x400 >;
			clocks = < 0x02 0x03 0x4000 >;
			interrupts = < 0x25 0x00 >;
			status = "okay";
			label = "UART_1";
			current-speed = < 0x1c200 >;
		};

		usart2: serial@40004400 {
			compatible = "st,stm32-usart", "st,stm32-uart";
			reg = < 0x40004400 0x400 >;
			clocks = < 0x02 0x02 0x20000 >;
			interrupts = < 0x26 0x00 >;
			status = "okay";
			label = "UART_2";
			current-speed = < 0x1c200 >;
		};

		usart3: serial@40004800 {
			compatible = "st,stm32-usart", "st,stm32-uart";
			reg = < 0x40004800 0x400 >;
			clocks = < 0x02 0x02 0x40000 >;
			interrupts = < 0x27 0x00 >;
			status = "okay";
			label = "UART_3";
			current-speed = < 0x1c200 >;
		};

		i2c1: i2c@40005400 {
			compatible = "st,stm32-i2c-v1";
			clock-frequency = < 0x61a80 >;
			#address-cells = < 0x01 >;
			#size-cells = < 0x00 >;
			reg = < 0x40005400 0x400 >;
			clocks = < 0x02 0x02 0x200000 >;
			interrupts = < 0x1f 0x00 >, < 0x20 0x00 >;
			interrupt-names = "event", "error";
			status = "okay";
			label = "I2C_1";
		};

		i2c2: i2c@40005800 {
			compatible = "st,stm32-i2c-v1";
			clock-frequency = < 0x61a80 >;
			#address-cells = < 0x01 >;
			#size-cells = < 0x00 >;
			reg = < 0x40005800 0x400 >;
			clocks = < 0x02 0x02 0x400000 >;
			interrupts = < 0x21 0x00 >, < 0x22 0x00 >;
			interrupt-names = "event", "error";
			status = "okay";
			label = "I2C_2";
		};

		spi1: spi@40013000 {
			compatible = "st,stm32-spi";
			#address-cells = < 0x01 >;
			#size-cells = < 0x00 >;
			reg = < 0x40013000 0x400 >;
			clocks = < 0x02 0x03 0x1000 >;
			interrupts = < 0x23 0x05 >;
			status = "okay";
			label = "SPI_1";
		};

		iwdg: watchdog@40003000 {
			compatible = "st,stm32-watchdog";
			reg = < 0x40003000 0x400 >;
			label = "IWDG";
			status = "disabled";
		};

		wwdg: watchdog@40002c00 {
			compatible = "st,stm32-window-watchdog";
			reg = < 0x40002c00 0x400 >;
			clocks = < 0x02 0x02 0x800 >;
			label = "WWDG";
			interrupts = < 0x00 0x07 >;
			status = "disabled";
		};

		timers1: timers@40012c00 {
			compatible = "st,stm32-timers";
			reg = < 0x40012c00 0x400 >;
			clocks = < 0x02 0x03 0x800 >;
			status = "okay";
			label = "TIMERS_1";

			pwm {
				compatible = "st,stm32-pwm";
				status = "okay";
				st,prescaler = < 0x2710 >;
				label = "PWM_1";
				#pwm-cells = < 0x02 >;
			};
		};

		timers2: timers@40000000 {
			compatible = "st,stm32-timers";
			reg = < 0x40000000 0x400 >;
			clocks = < 0x02 0x02 0x01 >;
			status = "disabled";
			label = "TIMERS_2";

			pwm {
				compatible = "st,stm32-pwm";
				status = "disabled";
				st,prescaler = < 0x00 >;
				label = "PWM_2";
				#pwm-cells = < 0x02 >;
			};
		};

		timers3: timers@40000400 {
			compatible = "st,stm32-timers";
			reg = < 0x40000400 0x400 >;
			clocks = < 0x02 0x02 0x02 >;
			status = "disabled";
			label = "TIMERS_3";

			pwm {
				compatible = "st,stm32-pwm";
				status = "disabled";
				st,prescaler = < 0x2710 >;
				label = "PWM_3";
				#pwm-cells = < 0x02 >;
			};
		};

		timers4: timers@40000800 {
			compatible = "st,stm32-timers";
			reg = < 0x40000800 0x400 >;
			clocks = < 0x02 0x02 0x04 >;
			status = "disabled";
			label = "TIMERS_4";

			pwm {
				compatible = "st,stm32-pwm";
				status = "disabled";
				st,prescaler = < 0x2710 >;
				label = "PWM_4";
				#pwm-cells = < 0x02 >;
			};
		};

		adc1: adc@40012400 {
			compatible = "st,stm32-adc";
			reg = < 0x40012400 0x400 >;
			clocks = < 0x02 0x03 0x200 >;
			interrupts = < 0x12 0x00 >;
			status = "disabled";
			label = "ADC_1";
			#io-channel-cells = < 0x01 >;
		};

		dma1: dma@40020000 {
			compatible = "st,stm32-dma";
			#dma-cells = < 0x04 >;
			reg = < 0x40020000 0x400 >;
			clocks = < 0x02 0x00 0x01 >;
			interrupts = < 0x0b 0x00 0x0c 0x00 0x0d 0x00 0x0e 0x00 0x0f 0x00 0x10 0x00 0x11 0x00 >;
			st,mem2mem;
			status = "disabled";
			label = "DMA_1";
		};

		spi2: spi@40003800 {
			compatible = "st,stm32-spi";
			#address-cells = < 0x01 >;
			#size-cells = < 0x00 >;
			reg = < 0x40003800 0x400 >;
			clocks = < 0x02 0x02 0x4000 >;
			interrupts = < 0x24 0x05 >;
			status = "okay";
			label = "SPI_2";
		};

		usb: usb@40005c00 {
			compatible = "st,stm32-usb";
			reg = < 0x40005c00 0x400 >;
			interrupts = < 0x14 0x00 >;
			interrupt-names = "usb";
			num-bidir-endpoints = < 0x08 >;
			ram-size = < 0x200 >;
			status = "okay";
			clocks = < 0x02 0x02 0x800000 >;
			phys = < 0x03 >;
			label = "USB";
		};
	};

	cpus {
		#address-cells = < 0x01 >;
		#size-cells = < 0x00 >;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-m3";
			reg = < 0x00 >;
		};
	};

	sram0: memory@20000000 {
		compatible = "mmio-sram";
		reg = < 0x20000000 0x5000 >;
	};

	usb_fs_phy: usbphy {
		compatible = "usb-nop-xceiv";
		#phy-cells = < 0x00 >;
		label = "USB_FS_PHY";
		phandle = < 0x03 >;
	};

	leds {
		compatible = "gpio-leds";

		led: led {
			gpios = < 0x04 0x0d 0x04 >;
			label = "LD";
		};
	};
};
